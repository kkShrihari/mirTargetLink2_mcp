{
  "name": "mcp-ts-simple-template",
  "version": "1.0.0",
  "description": "A simple TypeScript MCP server template for miRTargetLink 2",
  "author": "Shrihari",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc && chmod 755 build/index.js",
    "start": "node build/index.js",
    "start:build": "npm run build && npm start",
    "test": "node --loader ts-node/esm src/tests/test.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.6.1",
    "zod": "^3.25.76",
    "puppeteer": "^23.5.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
